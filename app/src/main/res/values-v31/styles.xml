<resources>

    <style name="Widget.APPFinalProject_10130492.AppWidget.Container" parent="android:Widget">
        <item name="android:id">@android:id/background</item>
        <item name="android:padding">?attr/appWidgetPadding</item>
        <item name="android:background">@drawable/app_widget_background</item>
        <item name="android:clipToOutline">true</item>
    </style>
    <style name="Widget.APPFinalProject_10130492.AppWidget.InnerTitle" parent="android:Widget">
        <!--<item name="android:background">@drawable/app_widget_inner_view_background</item>-->
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="android:clipToOutline">true</item>
    </style>
    <style name="Widget.APPFinalProject_10130492.AppWidget.InnerView" parent="android:Widget">
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="android:clipToOutline">true</item>
    </style>
    <style name="Widget.APPFinalProject_10130492.AppWidget.InnerViewCountIcon" parent="android:Widget">
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="colorControlNormal">?android:attr/colorForeground</item>
        <item name="android:padding">10dp</item>
        <item name="android:background">@drawable/app_widget_inner_view_count_down</item>

        <item name="android:clipToOutline">true</item>
    </style>
    <style name="Widget.APPFinalProject_10130492.AppWidget.InnerViewCountText" parent="android:Widget">
        <item name="android:textColor">?android:attr/colorForeground</item>
        <item name="colorControlNormal">?android:attr/colorForeground</item>
        <item name="android:background">@drawable/app_widget_inner_view_count_down_text</item>
        <item name="android:paddingEnd">10dp</item>
        <item name="android:clipToOutline">true</item>
    </style>
</resources>